{
  "name": "Album",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "unit-tests": "jest",
    "lint": "eslint .",
    "postinstall": "jetify"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^5.7.0",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.25",
    "@types/react-native": "^0.61.23",
    "@types/react-native-snap-carousel": "^3.8.1",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.19.2",
    "lodash": "^4.17.15",
    "numbro": "^2.2.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-cached-image": "https://github.com/fungilation/react-native-cached-image.git",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-navigation": "^6.3.1",
    "react-native-snap-carousel": "^3.9.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "^0.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/runtime": "7.9.2",
    "@react-native-community/eslint-config": "0.0.5",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.4",
    "@types/redux-logger": "^3.0.7",
    "babel-jest": "25.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "6.8.0",
    "jest": "^25.0.0",
    "jetifier": "^1.6.5",
    "metro-react-native-babel-preset": "0.56.4",
    "prettier": "^2.0.4",
    "prettier-eslint": "^8.8.2",
    "react-dom": "^16.13.1",
    "react-test-renderer": "16.9.0",
    "reactotron-react-native": "^4.0.3",
    "reactotron-redux": "^3.1.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest.setup.js",
      "<rootDir>/enzyme.config.ts",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "__tests__/TestUtils.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg||jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.ts",
      "^app/(.*)": "<rootDir>/src/$1",
      "^assets/(.*)": "<rootDir>/assets/$1"
    },
    "modulePathIgnorePatterns": [],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/__tests__/TestUtils.ts"
    ],
    "cacheDirectory": ".jest/cache",
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json",
        "babelConfig": "<rootDir>/babel.config.js"
      }
    }
  }
}
